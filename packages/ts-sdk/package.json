{
  "name": "@reelix/ts-sdk",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "main": "src/index.ts",
  "types": "src/index.ts",
  "files": ["src"],
  "exports": { ".": { "types": "./src/index.ts", "import": "./src/index.ts" } },
  "sideEffects": false,
  "scripts": {
    "generate": "orval --input ../schemas/openapi.yaml --output src/index.ts --mode single --client fetch --clean",
    "dev": "orval --input ../schemas/openapi.yaml --output src/index.ts --mode single --client fetch --clean --watch",
    "build": "pnpm run generate",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "orval": "7.11.2",
    "typescript": "^5.6.2"
  }
}
